<?xml version="1.0"?>
<extension key="de.systopia.statustracker" type="module">
  <file>statustracker</file>
  <name>Status Tracker</name>
  <description>Provides light-weight project or process tracking per contact</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.statustracker</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.statustracker</url>
    <url desc="Support">https://github.com/systopia/de.systopia.statustracker/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2019-11-22</releaseDate>
  <version>0.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Statustracker</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Statustracker_Upgrader</upgrader>
</extension>
